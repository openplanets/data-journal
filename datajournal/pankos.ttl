                           rdfs:subClassOf pankos:Facility .



###  http://www.purl.org/pankos#EnergyValuePartition

pankos:EnergyValuePartition rdf:type :Class ;
                            
                            :equivalentClass [ rdf:type :Class ;
                                               :unionOf ( pankos:HighEnergy
                                                          pankos:LowEngergy
                                                          pankos:MediumEnergy
                                                        )
                                             ] ;
                            
                            rdfs:subClassOf pankos:ValuePartition .



###  http://www.purl.org/pankos#Facility

pankos:Facility rdf:type :Class ;
                
                rdfs:subClassOf :Thing ;
                
                :disjointWith pankos:Technique .



